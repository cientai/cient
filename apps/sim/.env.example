# =============================================================================
# REQUIRED SERVER VARIABLES
# =============================================================================

# Database
DATABASE_URL="postgresql://postgres:password@localhost:5432/postgres"

# Authentication
BETTER_AUTH_URL=http://localhost:3000
BETTER_AUTH_SECRET=your_secret_key  # Use `openssl rand -hex 32` to generate
ENCRYPTION_KEY=your_encryption_key  # Use `openssl rand -hex 32` to generate
INTERNAL_API_SECRET=your_internal_api_secret  # Use `openssl rand -hex 32` to generate

# =============================================================================
# OPTIONAL SERVER VARIABLES
# =============================================================================

# Database (Alternative)
# POSTGRES_URL="postgresql://postgres:password@localhost:5432/postgres"

# Registration Control
# DISABLE_REGISTRATION=false

# =============================================================================
# BILLING & SUBSCRIPTION (STRIPE)
# =============================================================================

# Stripe Configuration
# STRIPE_SECRET_KEY=sk_test_...
# STRIPE_BILLING_WEBHOOK_SECRET=whsec_...
# STRIPE_WEBHOOK_SECRET=whsec_...

# Stripe Price IDs
# STRIPE_FREE_PRICE_ID=price_...
# STRIPE_PRO_PRICE_ID=price_...
# STRIPE_TEAM_PRICE_ID=price_...
# STRIPE_ENTERPRISE_PRICE_ID=price_...

# Cost Limits (in cents)
# FREE_TIER_COST_LIMIT=1000
# PRO_TIER_COST_LIMIT=10000
# TEAM_TIER_COST_LIMIT=50000
# ENTERPRISE_TIER_COST_LIMIT=100000

# =============================================================================
# EMAIL & NOTIFICATIONS
# =============================================================================

# Resend Email Service
# RESEND_API_KEY=re_...
# EMAIL_DOMAIN=yourdomain.com

# =============================================================================
# AI PROVIDERS
# =============================================================================

# OpenAI
# OPENAI_API_KEY=sk-...
# OPENAI_API_KEY_1=sk-...
# OPENAI_API_KEY_2=sk-...
# OPENAI_API_KEY_3=sk-...

# Azure OpenAI
# AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
# AZURE_OPENAI_API_VERSION=2024-02-15-preview

# Anthropic
# ANTHROPIC_API_KEY_1=sk-ant-...
# ANTHROPIC_API_KEY_2=sk-ant-...
# ANTHROPIC_API_KEY_3=sk-ant-...

# Mistral
# MISTRAL_API_KEY=your_mistral_api_key

# Freestyle
# FREESTYLE_API_KEY=your_freestyle_api_key

# Ollama (Local)
# OLLAMA_URL=http://localhost:11434

# =============================================================================
# MONITORING & TELEMETRY
# =============================================================================

# Sentry
# SENTRY_ORG=your_org
# SENTRY_PROJECT=your_project
# SENTRY_AUTH_TOKEN=your_auth_token

# Telemetry
# TELEMETRY_ENDPOINT=https://your-telemetry-endpoint.com
# COST_MULTIPLIER=1.0

# =============================================================================
# BROWSER AUTOMATION
# =============================================================================

# Browserbase
# BROWSERBASE_API_KEY=your_browserbase_api_key
# BROWSERBASE_PROJECT_ID=your_project_id

# =============================================================================
# STORAGE (AWS S3)
# =============================================================================

# AWS S3
# AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your_access_key_id
# AWS_SECRET_ACCESS_KEY=your_secret_access_key
# S3_BUCKET_NAME=your-bucket-name
# S3_LOGS_BUCKET_NAME=your-logs-bucket-name
# S3_KB_BUCKET_NAME=your-kb-bucket-name

# =============================================================================
# STORAGE (AZURE BLOB)
# =============================================================================

# Azure Blob Storage
# AZURE_ACCOUNT_NAME=your_account_name
# AZURE_ACCOUNT_KEY=your_account_key
# AZURE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=...
# AZURE_STORAGE_CONTAINER_NAME=your-container-name
# AZURE_STORAGE_KB_CONTAINER_NAME=your-kb-container-name

# =============================================================================
# MISCELLANEOUS
# =============================================================================

# Cron Jobs
# CRON_SECRET=your_cron_secret

# Log Retention
# FREE_PLAN_LOG_RETENTION_DAYS=7

# GitHub
# GITHUB_TOKEN=ghp_...

# ElevenLabs (Text-to-Speech)
# ELEVENLABS_API_KEY=your_elevenlabs_api_key

# JWT
# JWT_SECRET=your_jwt_secret

# =============================================================================
